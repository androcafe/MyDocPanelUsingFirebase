<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="visitindia.androcafe.mydocpanelusingfirebase">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".doctorsappointment.SplashActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".doctorsappointment.LoginActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:noHistory="true"/>
        <activity
            android:name=".doctorsappointment.RegisterActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".doctorsappointment.HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.NoActionBar"
            android:noHistory="true"/>

        <activity android:name=".doctorsappointment.home.FindDoctorsActivity"></activity>

        <activity android:name=".doctorsappointment.MyAppointmentActivity"></activity>
    </application>

</manifest>